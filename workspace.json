{
  "version": 2,
  "projects": {
    "express-app": {
      "root": "apps/express-app",
      "sourceRoot": "apps/express-app/src",
      "projectType": "application",
      "prefix": "express-app",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/express-app",
            "main": "apps/express-app/src/main.ts",
            "tsConfig": "apps/express-app/tsconfig.app.json",
            "assets": ["apps/express-app/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/express-app/src/environments/environment.ts",
                  "with": "apps/express-app/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": { "buildTarget": "express-app:build" }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/express-app/**/*.ts"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/express-app"],
          "options": {
            "jestConfig": "apps/express-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "next-app": {
      "root": "apps/next-app",
      "sourceRoot": "apps/next-app",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/next-app",
            "outputPath": "dist/apps/next-app"
          },
          "configurations": { "production": {} }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": { "buildTarget": "next-app:build", "dev": true, "port": 3051 },
          "configurations": {
            "production": {
              "buildTarget": "next-app:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": { "buildTarget": "next-app:build:production" }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/next-app/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/next-app"],
          "options": {
            "jestConfig": "apps/next-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "next-app-e2e": {
      "root": "apps/next-app-e2e",
      "sourceRoot": "apps/next-app-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/next-app-e2e/cypress.json",
            "tsConfig": "apps/next-app-e2e/tsconfig.e2e.json",
            "devServerTarget": "next-app:serve"
          },
          "configurations": {
            "production": { "devServerTarget": "next-app:serve:production" }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/next-app-e2e/**/*.{js,ts}"] }
        }
      }
    },
    "micro-app": {
      "root": "apps/micro-app",
      "sourceRoot": "apps/micro-app/src",
      "projectType": "application",
      "prefix": "micro-app",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/micro-app",
            "main": "apps/micro-app/src/main.ts",
            "tsConfig": "apps/micro-app/tsconfig.app.json",
            "assets": ["apps/micro-app/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/micro-app/src/environments/environment.ts",
                  "with": "apps/micro-app/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": { "buildTarget": "micro-app:build" }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/micro-app/**/*.ts"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/micro-app"],
          "options": {
            "jestConfig": "apps/micro-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "config": {
      "root": "libs/config",
      "sourceRoot": "libs/config/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["libs/config/**/*.ts"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/config"],
          "options": {
            "jestConfig": "libs/config/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": { "defaultCollection": "@nrwl/express" },
  "defaultProject": "express-app",
  "generators": {
    "@nrwl/react": { "application": { "babel": true } },
    "@nrwl/next": { "application": { "style": "css", "linter": "eslint" } }
  }
}
